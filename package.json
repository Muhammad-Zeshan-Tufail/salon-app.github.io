  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "optionalDependencies": {
    "typescript": "4.2.4"
  },
  "devDependencies": {
    "gh-pages": "^4.0.0",
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.9",
    "react-error-overlay": "^6.0.9"
  }
}
